{
  "version": 2,
  "builds": [
    {
      "src": "api/api.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/api.js"
    },
    {
      "src": "/(.*)",
      "dest": ".svelte-kit/output/$1"
    }
  ]
}